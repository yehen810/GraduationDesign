<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="line">
    <stroke
        android:width="1dp"
        android:color="#00FF00"
        android:dashWidth="5dp"
        android:dashGap="5dp" />

    <!--stroke 标签来设置线条的属性，dashWidth为虚线宽度，dashGap为虚线间隔-->
</shape>
